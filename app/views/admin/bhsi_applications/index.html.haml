= render 'collection_controls'

- content_for :top_links do
  = link_to 'Download Excel', admin_bhsi_applications_path(:format => 'xls', :year => params[:year])

= ssp_search_form BhsiApplication

.table_section
  %h3
    #{ params[:year].present? ? params[:year] : "2013" } BHSI Applications
    = new_model_link BhsiApplication
  .table_wrapper
    = render 'admin/bhsi_applications/table.html.haml', :bhsi_applications => @bhsi_applications
  = paginate @bhsi_applications
