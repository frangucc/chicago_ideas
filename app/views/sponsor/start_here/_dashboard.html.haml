  .text-share
    %p
      %trong Let's make an impact together
    %p Help share your experience with CIW on social media. Please get involed
  .share-icon
    = link_to "facebook", "#", class: 'fb'
    = link_to "twitter", "#", class: 'tw'
    = link_to "in", "#", class: 'in'
    = link_to "google plus", "#", class: 'gp'
